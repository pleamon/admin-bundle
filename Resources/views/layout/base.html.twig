<!DOCTYPE html>
<html>
<head>

    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>{% block title %}p admin{% endblock %}</title>

    {% stylesheets filter="cssrewrite"
    '@PAdminBundle/Resources/public/css/bootstrap.min.css'
    '@PAdminBundle/Resources/public/font-awesome/css/font-awesome.min.css'
    '@PAdminBundle/Resources/public/css/plugins/select2/select2.min.css'
    '@PAdminBundle/Resources/public/css/plugins/sweetalert/sweetalert.css'
    '@PAdminBundle/Resources/public/css/plugins/summernote/summernote.css'
    '@PAdminBundle/Resources/public/css/plugins/summernote/summernote-bs3.css'
    '@PAdminBundle/Resources/public/css/animate.css'
    '@PAdminBundle/Resources/public/css/style.css'
    '@PAdminBundle/Resources/public/css/common.css'
    %}
    <link rel="stylesheet" type="text/css" href="{{ asset(asset_url) }}" />
    {% endstylesheets %}

    {% javascripts
    '@PAdminBundle/Resources/public/js/jquery-2.1.1.js'
    '@PAdminBundle/Resources/public/js/jquery-ui-1.10.4.min.js'
    %}
    <script type="text/javascript" src="{{ asset(asset_url) }}"></script>
    {% endjavascripts %}

</head>
<body>

<div id="wrapper">
    <nav class="navbar-default navbar-static-side" role="navigation">
        <div class="sidebar-collapse">
            <ul class="nav metismenu" id="side-menu">
                <li class="nav-header">
                    <div class="dropdown profile-element">
                        <a data-toggle="dropdown" class="dropdown-toggle" href="#">
                            <span class="clear">
                                <span class="block m-t-xs">
                                    <strong class="font-bold">{% block brand_title %}{% endblock %}</strong>
                                </span>
                                <span class="text-muted text-xs block">
                                    {% block brand_desc %}Art Director{% endblock %}
                                    <b class="caret"></b>
                                </span>
                            </span>
                        </a>
                        <ul class="dropdown-menu animated fadeInRight m-t-xs">
                            {% block brand_list %}
                            <li><a href="#">Logout</a></li>
                            {% endblock %}
                        </ul>
                    </div>
                    <div class="logo-element">
                        {% block admin_title %}
                        {{ admin_title }}
                        {% endblock %}
                    </div>
                </li>
                {% block menus %}
                {% endblock %}
            </ul>
        </div>
    </nav>
    <div id="page-wrapper" class="gray-bg">
        {% block navbar %}
        <div class="row border-bottom">
            <nav class="navbar navbar-static-top" role="navigation" style="margin-bottom: 0">
                <div class="navbar-header">
                    <a class="navbar-minimalize minimalize-styl-2 btn btn-primary " href="#"><i class="fa fa-bars"></i> </a>
                    <form role="search" class="navbar-form-custom" method="post" action="{{ admin_search.route and path(admin_search.route) or '' }}">
                        <div class="form-group">
                            <input type="text" placeholder="{{ admin_search.text }}" class="form-control" name="top-search" id="top-search">
                        </div>
                    </form>
                </div>
                <ul class="nav navbar-top-links navbar-right">
                    {% block navbar_right %}
                    {% endblock %}
                    {% block navbar_defined %}
                    <li> <a href="{{ path('fos_user_security_logout') }}"> <i class="fa fa-sign-out"></i> Log out </a> </li>
                    <li> <a class="right-sidebar-toggle"> <i class="fa fa-tasks"></i> </a> </li>
                    {% endblock %}
                </ul>
            </nav>
        </div>
        {% endblock %}

        {% block page_heading %}
        <div class="row wrapper border-bottom white-bg page-heading">
            {% block page_heading_content %}
            {% endblock %}
        </div>
        {% endblock %}

        <div class="wrapper wrapper-content animated fadeInRight">
            {% block content %}{% endblock %}
        </div>
        <div class="footer">
            {% block footer %}{% endblock %}
        </div>

        {% block sidebar %}
        <div id="right-sidebar">
            <div class="sidebar-container">
                <ul class="nav nav-tabs navs-{% block sidebar_tab_count %}3{% endblock %}">
                    {% block sidebar_tab %}{% endblock %}
                </ul>

                <div class="tab-content">
                    {% block sidebar_content %}{% endblock %}
                </div>
            </div>
        </div>
        {% endblock %}
    </div>
    {% for i in 1..padmin.getParameter("p.admin.modal").count %}
    <div class="modal inmodal in" id="modal{{ i }}" role="dialog" aria-hidden="true">
        <div class="modal-dialog">
            <div class="modal-content animated fadeIn">
            </div>
        </div>
    </div>
    {% endfor %}
</div>

<!-- Mainly scripts -->
    {% javascripts
    '@PAdminBundle/Resources/public/js/bootstrap.min.js'
    '@PAdminBundle/Resources/public/js/plugins/metisMenu/jquery.metisMenu.js'
    '@PAdminBundle/Resources/public/js/plugins/slimscroll/jquery.slimscroll.min.js'
    '@PAdminBundle/Resources/public/js/inspinia.js'
    '@PAdminBundle/Resources/public/js/plugins/pace/pace.min.js'
    '@PAdminBundle/Resources/public/js/plugins/select2/select2.full.min.js'
    '@PAdminBundle/Resources/public/js/plugins/sweetalert/sweetalert.min.js'
    '@PAdminBundle/Resources/public/js/plugins/summernote/summernote.min.js'
    '@PAdminBundle/Resources/public/js/padmin.js'
    %}
    <script type="text/javascript" src="{{ asset(asset_url) }}"></script>
    {% endjavascripts %}

    {% block javascripts %}
    {% endblock %}
</body>
</html>
