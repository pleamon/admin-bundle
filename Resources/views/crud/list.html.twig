{% extends 'PAdminBundle:layout:layout.html.twig' %}

{% block content -%}
<div class="ibox">
    <div class="ibox-title">
        <h5>{% block ibox_title %}{% endblock %}</h5>
        {% block ibox_tools %}
        <div class="ibox-tools">
            {% block ibox_tools_content %}
            {% endblock %}
        </div>
        {% endblock %}
    </div>
    <div class="ibox-content">
        {% block ibox_content %}
        {% endblock %}
    </div>
</div>
{% endblock %}
